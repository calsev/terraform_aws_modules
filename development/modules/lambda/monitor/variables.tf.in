variable "alert_level_default" {
  type    = string
  default = "general_medium"
}

variable "function_map" {
  type = map(object({
    {{ alarm.var_item() }}
    {{ name.var_item() }}
    name_effective = string # The name of the lambda
  }))
}

{{ lambda.var_alarm_default() }}

variable "monitor_data" {
  type = object({
    alert = object({
      topic_map = map(object({
        topic_arn = string
      }))
    })
  })
}

{{ name.var() }}

{{ std.map() }}
